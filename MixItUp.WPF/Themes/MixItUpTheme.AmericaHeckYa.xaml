<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"> 
  <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#DD800000" po:Freeze="True" />                   <!-- Emphasis Background -->
  <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="#DDFFFFFF" po:Freeze="True" />         <!-- Emphasis Text -->
  <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="#DD002D82" po:Freeze="True" />                     <!-- Main Background -->
  <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="#DD404040" po:Freeze="True" />           <!-- Main Foreground -->
  <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="#DD001338" po:Freeze="True" />                    <!-- Highlight Background -->
  <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="#DD404040" po:Freeze="True" />          <!-- Unknown... -->

  <SolidColorBrush x:Key="MainApplicationBackground" Color="Black" po:Freeze="True" />                  <!-- Application Background -->
</ResourceDictionary>