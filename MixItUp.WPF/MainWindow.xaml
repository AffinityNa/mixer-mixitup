<Window x:Class="MixItUp.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MixItUp.WPF"
        xmlns:ChatControls="clr-namespace:MixItUp.WPF.Controls.Chat"
        xmlns:InteractiveControls="clr-namespace:MixItUp.WPF.Controls.Interactive"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TabControl>
            <TabItem Header="Chat">
                <ChatControls:ChatControl x:Name="Chat" />
            </TabItem>
            <TabItem Header="Interactive">
                <InteractiveControls:InteractiveControl x:Name="Interactive" />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
